{
  "name": "mawejs",
  "version": "0.23.0",
  "description": "Editor for writers.",
  "author": "Markus Koskimies <mkoskim@gmail.com>",
  "scripts": {
    "dev": "electron-vite -w dev",
    "build": "npm run vite-build && electron-builder",
    "vite-build": "electron-vite build",
    "preview": "electron-vite preview",
    "audit": "npm audit --omit=dev",
    "fresh": "rm -rf dist build node_modules package-lock.json && npm install",
    "fix": "sudo chown root node_modules/electron/dist/chrome-sandbox && sudo chmod 4755 node_modules/electron/dist/chrome-sandbox"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@mdi/js": "^7.4.47",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "electron-devtools-installer": "^4.0.0",
    "electron-localshortcut": "^3.2.1",
    "electron-window-state": "^5.0.3",
    "fs-extra": "^11.3.3",
    "immer": "^11.1.4",
    "is-gzip": "^2.0.0",
    "mammoth": "^1.11.0",
    "material-ui-popup-state": "^5.3.6",
    "nanoid": "^5.1.6",
    "notistack": "^3.0.2",
    "pako": "^2.1.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-infinite-scroll-component": "^6.1.1",
    "recharts": "^3.7.0",
    "slate": "^0.123.0",
    "slate-dom": "^0.123.0",
    "slate-history": "^0.113.1",
    "slate-react": "^0.123.0",
    "stream": "^0.0.3",
    "use-immer": "^0.11.0",
    "uuid": "^13.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.4",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "electron-vite": "^5.0.0",
    "node": "^25.6.1"
  },
  "main": "./out/main/index.js",
  "build": {
    "files": [
      "out/**/*",
      "package.json"
    ],
    "extraResources": [
      "examples/tutorial/*"
    ],
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
